{
  "name": "@lzptec/concurrency",
  "version": "0.0.1-1",
  "description": "A lightweight async context manager for typescript and javascript",
  "author": "Andr√© Posso <admin@lzptec.com>",
  "license": "MIT",
  "keywords": [
    "Context",
    "Context-Manager",
    "Proxy",
    "typescript",
    "javascript",
    "async"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "tsup src/index.ts --dts --clean",
    "prepublish": "pnpm run build",
    "tsc": "tsc",
    "test": "echo \"No test specified\""
  },
  "devDependencies": {
    "@types/node": "^16.11.00",
    "ava": "^4.3.3",
    "tslib": "^2.4.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  }
}